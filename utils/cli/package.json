{
	"name": "@fontsource-utils/cli",
	"version": "0.2.0-beta.9",
	"description": "Fontsource CLI",
	"bin": {
		"fontsource": "./dist/cli.mjs"
	},
	"main": "./dist/index.cjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.cts",
	"exports": {
		"require": {
			"types": "./dist/index.d.cts",
			"default": "./dist/index.cjs"
		},
		"import": {
			"types": "./dist/index.d.mts",
			"default": "./dist/index.mjs"
		}
	},
	"scripts": {
		"build": "pkgroll --target=node14",
		"dev": "pnpm run build && node ./dist/cli.mjs",
		"lint": "eslint --fix",
		"coverage": "vitest run --coverage",
		"test": "vitest",
		"prepublish": "pnpm run build"
	},
	"keywords": [
		"fontsource",
		"fonts",
		"google fonts"
	],
	"author": "Ayuhito <hello@ayuhito.com>",
	"license": "MIT",
	"dependencies": {
		"@clack/prompts": "^0.6.3",
		"@fontsource-utils/generate": "workspace:*",
		"cac": "^6.7.14",
		"consola": "^3.1.0",
		"dotenv": "^16.0.3",
		"flat": "^5.0.2",
		"fs-extra": "^11.1.1",
		"google-font-metadata": "^5.1.0",
		"got": "^12.6.0",
		"json-stringify-pretty-compact": "^4.0.0",
		"p-queue": "^7.3.4",
		"pathe": "^1.1.0",
		"picocolors": "^1.0.0"
	},
	"devDependencies": {
		"@ayuhito/eslint-config": "^0.2.4",
		"@types/flat": "^5.0.2",
		"@types/fs-extra": "^11.0.1",
		"@types/node": "^18.16.2",
		"@vitest/coverage-c8": "^0.30.1",
		"eslint": "^8.39.0",
		"pkgroll": "^1.9.0",
		"tsx": "^3.12.6",
		"typescript": "^5.0.4",
		"vitest": "^0.30.1"
	},
	"files": [
		"dist/*"
	],
	"publishConfig": {
		"access": "public"
	},
	"homepage": "https://github.com/fontsource/fontsource/tree/main/utils/cli",
	"repository": {
		"type": "git",
		"url": "https://github.com/fontsource/fontsource.git",
		"directory": "utils/cli"
	}
}
