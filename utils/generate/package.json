		"@types/node": "^18.14.1",
		"c8": "^7.13.0",
		"eslint": "^8.34.0",
		"pkgroll": "^1.9.0",
		"tsx": "^3.12.3",
		"typescript": "^4.9.5",
		"vitest": "^0.29.1"
	},
	"files": [
		"dist/*"
	],
	"publishConfig": {
		"access": "public"
