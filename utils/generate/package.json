{
	"name": "@fontsource-utils/generate",
	"version": "0.1.3",
	"description": "A CSS font-face generator",
	"main": "./dist/index.cjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.cts",
	"exports": {
