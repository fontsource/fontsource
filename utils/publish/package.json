{
	"name": "@fontsource-utils/publish",
	"version": "0.1.0",
	"description": "Fontsource Publish CLI",
	"bin": {
		"fontsource-publish": "./dist/cli.mjs"
	},
	"main": "./dist/index.cjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.cts",
	"exports": {
		"require": {
			"types": "./dist/index.d.cts",
			"default": "./dist/index.cjs"
		},
		"import": {
			"types": "./dist/index.d.mts",
			"default": "./dist/index.mjs"
		}
	},
	"scripts": {
		"build": "pkgroll --target=node14",
		"dev": "pnpm run build && node ./dist/cli.mjs",
		"lint": "eslint --fix",
		"coverage": "vitest run --coverage",
		"test": "vitest"
	},
	"keywords": [],
	"author": "Ayuhito <hello@ayuhito.com>",
	"license": "MIT",
	"dependencies": {
		"@types/libnpmpublish": "^4.0.3",
		"@types/node": "^18.6.4",
		"@npmcli/package-json": "^2.0.0",
		"cac": "^6.7.12",
		"consola": "^2.15.3",
		"defu": "^6.1.0",
		"dotenv": "^16.0.1",
		"execa": "^6.1.0",
		"json-stringify-pretty-compact": "^4.0.0",
		"latest-version": "^7.0.0",
		"libnpmpublish": "^6.0.4",
		"p-queue": "^7.3.0",
		"pacote": "^13.6.1",
		"parse-git-config": "^3.0.0",
		"pathe": "^0.3.3",
		"picocolors": "^1.0.0",
		"prompts": "^2.4.2",
		"semver": "^7.3.7"
	},
	"devDependencies": {
		"@ayuhito/eslint-config": "^0.2.4",
		"@types/node": "^18.14.1",
		"@vitest/coverage-c8": "^0.29.1",
		"eslint": "^8.34.0",
		"pkgroll": "^1.9.0",
		"tsx": "^3.12.3",
		"typescript": "^4.9.5",
		"vitest": "^0.29.1"
	},
	"files": [
		"dist/*"
	],
	"publishConfig": {
		"access": "public"
	},
	"homepage": "https://github.com/fontsource/fontsource/tree/main/utils/publish",
	"repository": {
		"type": "git",
		"url": "https://github.com/fontsource/fontsource.git",
		"directory": "utils/publish"
