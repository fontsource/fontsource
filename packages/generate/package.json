{
	"name": "@fontsource-utils/generate",
	"version": "0.3.1",
	"description": "A CSS font-face generator",
	"main": "./dist/index.cjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.cts",
	"exports": {
		"require": {
			"types": "./dist/index.d.cts",
			"default": "./dist/index.cjs"
		},
		"import": {
			"types": "./dist/index.d.mts",
			"default": "./dist/index.mjs"
		}
	},
	"scripts": {
		"build": "pkgroll --target=node14",
		"lint": "eslint . --fix",
		"coverage": "vitest run --coverage",
		"test": "vitest",
		"prepublish": "pnpm run build",
		"ci:lint": "eslint ."
	},
	"keywords": [],
	"author": "Ayuhito <hello@ayuhito.com>",
	"license": "MIT",
	"devDependencies": {
		"@ayuhito/eslint-config": "^0.4.1",
		"@types/node": "^20.4.7",
		"eslint": "^8.46.0",
		"pkgroll": "^1.9.0",
		"tsx": "^3.12.3",
		"typescript": "^5.1.6"
	},
	"files": [
		"dist/*"
	],
	"publishConfig": {
		"access": "public"
	},
	"homepage": "https://github.com/fontsource/fontsource/tree/main/utils/generate",
	"repository": {
		"type": "git",
		"url": "https://github.com/fontsource/fontsource.git",
		"directory": "utils/generate"
	}
}
